# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
/tmp/pids/*
/tmp/cache/*
/tmp/sockets/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key
/config/credentials.yml.enc

# Ignore environment variables files.
/.env
/.env.*

# Ignore database files.
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.sqlite3-*
/db/development.sqlite3
/db/test.sqlite3

# Ignore node_modules (for webpacker / js bundling).
/node_modules

# Ignore precompiled assets.
/public/assets
/public/packs
/public/packs-test
/public/uploads
/public/system

# Ignore yarn log files.
/yarn-error.log
.yarn-integrity

# Ignore coverage reports.
/coverage/

# Ignore OS generated files
.DS_Store
Thumbs.db

# Ignore byebug history
.byebug_history

# Ignore IDE/editor files
.idea/
.vscode/
*.swp
/app/assets/builds/*
!/app/assets/builds/.keep
